<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="GitgameshTest">
	<test name="GitgameshTest" enabled="true" preserve-order="true">
		<groups>
			<run>
				<exclude name="sshCommandExecutor" />
				<include name="gitClient" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.gitgamesh.core.git.ssh.test.SshCommandExecutorTest" />
			<class name="com.biit.gitgamesh.core.git.ssh.test.GitClientTest" />
		</classes>
	</test>
</suite>